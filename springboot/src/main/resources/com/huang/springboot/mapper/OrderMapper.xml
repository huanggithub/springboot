<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huang.springboot.mapper.OrderMapper">
   <insert id="add" parameterType="com.huang.springboot.pojo.Order">
       insert into tab_order (oid,order_time,order_total_price) values (#{oid},#{order_time},#{order_total_price})
   </insert>
</mapper>